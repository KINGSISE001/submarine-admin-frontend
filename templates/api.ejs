import request from '@/utils/request'

<%_ var entityKebab = StrUtil.camelToKebab(entity) -%>
/**
 * 分页获取
 */
export function get<%= Entity %>Page(params, pageNum, pageSize) {
  return request({
    url: '/<%= entityKebab %>/list/page',
    method: 'get',
    params: {
      ...params,
      pageNum,
      pageSize
    }
  })
}

/**
 * 不分页获取
 */
export function get<%= Entity %>List(params) {
  return request({
    url: '/<%= entityKebab %>/list/all',
    method: 'get',
    params
  })
}

/**
 * 获取详情
 */
export function get<%= Entity %>Detail(id) {
  return request({
    url: '/<%= entityKebab %>/detail',
    method: 'get',
    params: { id }
  })
}

/**
 * 保存
 */
export function save<%= Entity %>(data) {
  return request({
    url: '/<%= entityKebab %>/save',
    method: 'post',
    data
  })
}

/**
 * 删除
 */
export function delete<%= Entity %>(id) {
  return request({
    url: '/<%= entityKebab %>/del',
    method: 'delete',
    data: { id }
  })
}
